{{!-- some header --}}


{{!-- list of available workers --}}
{{#each workers}}
       <div class="row">
        <div class="col s12 m3">
            <div class="card">
                <div class="card-image">
                    <img src="../public/assets/images/jacob.jpg">
                    <span id="worker-name" class="card-title"></span>
                    <a class="btn-floating halfway-fab waves-effect waves-light green">
                        <i class="material-icons">add</i>
                    </a>
                </div>
                <div class="card-content">
                    <p id="skill">Service: {{this.service}} </p>
                    <p id="zipcode">Zipcode: {{this.zip_code}} </p>
                    <p id="phone">Phone: {{this.phone}}</p>
                    <p id="email">Email: {{this.email}}</p>
                </div>
            </div>
        </div>
    </div>
{{/each}}


<!--Import jQuery before materialize.js-->
<script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<script type="text/javascript" src="../public/assets/js/materialize.min.js"></script>
<script type="text/style" src="../public/assets/css/style.css"></script>









